<keymap version="1" name="Windows copy Proper Redo" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="ActivateCommitToolWindow">
    <keyboard-shortcut first-keystroke="alt 2" />
  </action>
  <action id="ActivateFavoritesToolWindow" />
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="ctrl t" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="EditorToggleUseSoftWraps">
    <keyboard-shortcut first-keystroke="ctrl e" second-keystroke="w" />
  </action>
  <action id="FocusEditor">
    <keyboard-shortcut first-keystroke="escape" />
    <keyboard-shortcut first-keystroke="ctrl g" />
  </action>
  <action id="GotoLine" />
  <action id="GotoRow" />
  <action id="RecentFiles" />
  <action id="SwitcherIterateItems" />
  <action id="SwitcherRecentEditedChangedToggleCheckBox" />
  <action id="Vcs.QuickListPopupAction" />
  <action id="Vcs.ShowMessageHistory">
    <keyboard-shortcut first-keystroke="ctrl m" />
  </action>
  <action id="Vcs.UpdateProject" />
</keymap>